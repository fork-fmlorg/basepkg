--- listpkgs	4 Jan 2006 13:35:55 -0000	1.12
+++ listpkgs	10 Sep 2012 21:48:57 -0000
@@ -46,4 +46,19 @@
 fi
 setname="$1"
 
+# We don't look for packages outside their named set, so make sure that
+# this set doesn't have anyone else's packages in it.
+badset=''
+for pkgname in $(list_set_files "${setname}" | ${AWK} '{print $2}' | grep -v "^${setname}-" | ${SORT} -u)
+do
+	list_set_files "${setname}" | fgrep >&2 -- "${pkgname}"
+	badset="${setname}"
+done
+
+if [ "$badset" != "" ]
+then
+    	echo >&2 "${setname}: set contains syspkg(s) that are not in ${setname}, aborting"
+	exit 1
+fi
+
 list_set_files "${setname}" | ${AWK} '{print $2}' | ${SORT} -u
